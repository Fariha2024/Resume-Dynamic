{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/resume dynamic"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
       "dest": "/public/$1",
      
    }
  ]
}


{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}

{
  "builds": [
    { "src": "index.html", "use": "@vercel/resume dynamic" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}